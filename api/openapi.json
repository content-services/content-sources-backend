            "api.ResponseMetadata": {
                "properties": {
                    "count": {
                        "description": "Total count of results",
                        "type": "integer"
                    },
                    "limit": {
                        "description": "Limit of results used for the request",
                        "type": "integer"
                    },
                    "offset": {
                        "description": "Offset into results used for the request",
                        "type": "integer"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "RhIdentity": {
                "in": "header",
                "name": "x-rh-identity",
                "type": "apiKey"
            }
        }
